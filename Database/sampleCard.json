NEW TABLE STRUCTURE 
-- =========================================
-- TABLE: roots
-- =========================================

CREATE TABLE IF NOT EXISTS roots (
  id           INTEGER PRIMARY KEY AUTOINCREMENT,
  root         TEXT NOT NULL,                      -- e.g. زهد
  family       TEXT NOT NULL,                      -- وزن / family (e.g. فَعِلَ)

  cards        JSON NOT NULL,                      -- cards array only

  status       TEXT NOT NULL DEFAULT 'active',     -- active | draft | reviewed | archived
  difficulty   INTEGER,                            -- 1–5
  frequency    TEXT,                               -- high | medium | low

  create_date  TEXT NOT NULL DEFAULT (datetime('now')),
  update_date  TEXT,
  extract_date TEXT,

  UNIQUE (root, family)
);

CREATE INDEX IF NOT EXISTS idx_roots_root       ON roots(root);
CREATE INDEX IF NOT EXISTS idx_roots_family     ON roots(family);
CREATE INDEX IF NOT EXISTS idx_roots_status     ON roots(status);
CREATE INDEX IF NOT EXISTS idx_roots_frequency  ON roots(frequency);


